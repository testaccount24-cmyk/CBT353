)ATTR
  @  TYPE(OUTPUT) INTENS(LOW)
)BODY
%-----------------     VTOC DISPLAY FACILITY (VDF) -----------------------------
 COMMAND ==>_VCMD                                               %SCROLL =>_AMT %
%                                   NO OF FREE  FREE  FREE  LRGST FREE   SIO
%SEL  VOLSER ADR  TYPE STATUS USAGE USERS CYLS  TRKS  EXTS    CYL  TRK COUNT
)MODEL
 _Z  @Z
)INIT
 .CURSOR=VCMD
  &AMT=PAGE
  &VCMD='    '
  &VARS='(VSLA VTOLINE)'
  .HELP=VDFHELP
)END
